{
  "name": "vue-state-stack",
  "version": "1.0.0",
  "description": "simple state stack for time travel operations",
  "main": "dist/state-stack.js",
  "module": "src/state-stack.js",
  "browser": "dist/state-stack.browser.js",
  "scripts": {
    "legacyVue": "npm i vue@^1.0.28",
    "latestVue": "npm i vue@latest",
    "test": "npm run test:run",
    "test:versions": "npm run legacyVue && npm run test:run && npm run latestVue && npm run test:run",
    "test:latest": "npm run latestVue && npm run test:run",
    "test:run": "babel-node test/run.jasmine.js && babel-node test/run.karma.js",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "karma": "node -r babel-register test/run.karma.js -w",
    "build": "rollup -c rollup.config.js && webpack",
    "doc": "cp README.md docs/intro.md && jsdoc2md src/state-stack.js > docs/api.md && gitbook install && gitbook build && npm run doc:replace",
    "doc:replace": "replace-in-file '/href=\"./\"/g' 'href=\"index.html\"' ./_book/*.html --isRegex "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dasdeck/state-stack.git"
  },
  "author": "Jan \"JM\" Manuel Schlieper",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "coveralls": "^3.0.0",
    "gitbook-cli": "^2.3.2",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^3.0.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "nyc": "^11.2.1",
    "replace-in-file": "^3.0.0-beta.2",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "standard": "^10.0.3",
    "vue": "^2.5.3",
    "webpack": "^3.6.0"
  }
}
